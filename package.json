{
  "name": "tidyjs-parser",
  "version": "0.1.1",
  "description": "A parser for imports",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "build-prod": "npm run check-types && npm run lint && node esbuild.js --production",
    "check-types": "tsc --noEmit --project tsconfig.json",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "ts-node src/test/runTest.ts",
    "unit:test": "npx jest test/unit --verbose",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc"
  },
  "keywords": [
    "typescript",
    "parser",
    "imports"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/generator": "^7.26.10",
    "@babel/parser": "^7.26.10",
    "@jest/globals": "^29.7.0",
    "@types/babel__generator": "^7.6.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "esbuild": "^0.25.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-output-reset": "^2.0.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2"
  }
}